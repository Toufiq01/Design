class ParkingSystem {
    int[] vehicle;
    public ParkingSystem(int big, int medium, int small) {
        vehicle = new int[]{big, medium, small};
    }
    
    public boolean addCar(int carType) {
        return vehicle[carType-1]-- > 0; //it's just a condition only return true and false
    }
}
// Runtime: 6 ms, faster than 99.71% of Java online submissions for Design Parking System.
// Memory Usage: 39.6 MB, less than 64.03% of Java online submissions for Design Parking System.
